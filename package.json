{
  "license": "MIT",
  "name": "portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/deeepak-me/portfolio"
  },
  "scripts": {
    "clean": "rimraf _site",
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "date-fns": "^2.16.1",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "source-map-explorer": "^2.5.1",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
